{
  "name": "@stephen-optisol/typeorm-audit",
  "version": "0.0.1",
  "description": "TypeORM global Audit: Create history of all tables and manage changes of those entity automatically.",
  "homepage": "https://github.com/stephen-optisol/typeorm-auditing",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stephen-optisol/typeorm-auditing.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "tsc -p tsconfig.build.json",
    "publish": "rm -rf dist && npm run build && npm publish --access public"
  },
  "keywords": [
    "TypeORM",
    "Decorator",
    "Audit",
    "History"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "mysql2": "^3.12.0",
    "prettier": "^3.4.2",
    "sqlite3": "^5.1.7",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "typeorm": "^0.3.20"
  },
  "peerDependencies": {
    "typeorm": "^0.3.20"
  },
  "files": [
    "dist/**/*"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "maxConcurrency": 1,
    "maxWorkers": 1
  },
  "bugs": {
    "url": "https://github.com/stephen-optisol/typeorm-auditing/issues"
  },
  "author": ""
}
