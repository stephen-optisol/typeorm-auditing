{
  "name": "typeorm-global-audit",
  "version": "0.0.2",
  "description": "TypeORM global Audit: Create history of all tables and manage changes of those entity automatically.",
  "homepage": "https://github.com/stephen-optisol/typeorm-auditing",
  "repository": {
    "type": "git",
    "url": "https://github.com/stephen-optisol/typeorm-auditing.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "tsc -p tsconfig.build.json",
    "publish": "rm -rf dist && npm run build && npm publish --access public"
  },
  "keywords": [
    "TypeORM",
    "Decorator",
    "Audit",
    "History"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "jest": "^28.1.3",
    "jest-junit": "^14.0.0",
    "mysql2": "^2.3.3",
    "prettier": "^2.7.1",
    "sqlite3": "^5.0.9",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "typeorm": "^0.3.0"
  },
  "peerDependencies": {
    "typeorm": "^0.3.0"
  },
  "files": [
    "dist/**/*"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "maxConcurrency": 1,
    "maxWorkers": 1
  }
}
